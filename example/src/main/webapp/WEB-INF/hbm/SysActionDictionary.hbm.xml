<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SysActionDictionary" table="sys_action_dictionary">
		<id name="id" type="java.lang.String">
            	<column name="ad_action"/>
            	<generator class="assigned" />
        	</id>
		<property name="label" column="ad_label" type="string" />
		<property name="parent" column="ad_parent" type="string" />
		<property name="isbottom" column="ad_isbottom" type="string" />
		<property name="partners" column="ad_partners" type="string" />
	</class>
</hibernate-mapping>